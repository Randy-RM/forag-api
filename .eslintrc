{
    "env": {
        "node": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "plugins": [
        "prettier"
    ],
    "rules": {
        "prettier/prettier": [
            "error",
            {
                "endOfLine": "auto"
            }
        ],
        "no-console": "off",
        "object-shorthand": "off",
        "no-return-await": "off",
        "no-unused-vars": [
            "error",
            {
                "argsIgnorePattern": "req|res|next|val|err|token"
            }
        ],
        "consistent-return": "off",
        "arrow-body-style": "off",
        "no-underscore-dangle": "off",
        "quotes": [
            "error",
            "single"
        ],
        "linebreak-style": "off"
    }
}