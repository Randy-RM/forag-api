{
  "name": "forag-api",
  "version": "1.0.0",
  "description": "backend of the survey and voting application",
  "main": "./src/app.js",
  "author": "Randy-Assani-Beni-Muhema",
  "license": "MIT",
  "keywords": [
    "survey application",
    "voting application",
    "backend survey application api",
    "backend voting application api"
  ],
  "scripts": {
    "start": "nodemon",
    "test": "jest --coverage",
    "prettier": "prettier --ignore-path .gitignore --write \"**/*.+(js|jsx|ts|tsx|json|css|html)\"",
    "format": "yarn prettier --write .",
    "check-format": "yarn prettier --list-different",
    "eslint": "eslint . --fix",
    "lint": "eslint --ignore-path .gitignore --ext .js,.jsx,.ts,.tsx .",
    "validate": "npm-run-all --parallel check-format lint test",
    "prepare": "husky install"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-validator": "^6.12.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mysql2": "^2.3.0",
    "sequelize": "^6.6.5"
  },
  "devDependencies": {
    "nodemon": "^2.0.7",
    "sequelize-cli": "^6.2.0",
    "eslint": "^8.0.1",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "husky": "^7.0.0",
    "jest": "^27.3.1",
    "prettier": "^2.4.1"
  }
